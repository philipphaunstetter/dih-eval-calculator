{
  "name": "Dynamic Table Calculator",
  "id": "com.hama.dynamic-table-calculator",
  "version": "1.0.0",
  "description": "Dynamic table with calculation capabilities",
  "main": "index.html",
  "permissions": [],
  "scopes": [
    "boards:read",
    "boards:write"
  ],
  "dependencies": {
    "@mirohq/miro-api": "^2.0.0"
  },
  "capabilities": {
    "canCreateContent": true
  },
  "extensionPoints": {
    "toolbar": {
      "icon": "toolbarIcon.svg",
      "title": "Dynamic Table Calculator",
      "toolbarItem": {
        "observes": ["selection"]
      }
    }
  }
} 